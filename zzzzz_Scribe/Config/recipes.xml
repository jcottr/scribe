	<setattribute xpath="/recipes/recipe[@name='cntSteelWritableCrate']" name="count">10</setattribute>

	<set xpath="/recipes/recipe[@name='cntWoodWritableCrate']">
		<ingredient name="FuriousRamsayWoodPlank" count="100" />
